        "c": "gcc '$fileName' -o './exe/$fileNameWithoutExt.exe' -Wall -O2 -m64 -lm -static-libgcc -std=c11 -fexec-charset=GBK && &'./exe/$fileNameWithoutExt.exe'",
        "cpp": "g++ '$fileName' -o './exe/$fileNameWithoutExt.exe' -Wall -O2 -m64 -static-libgcc -std=c++14 -fexec-charset=GBK && &'./exe/$fileNameWithoutExt.exe'",
